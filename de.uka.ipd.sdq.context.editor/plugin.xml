<plugin>


  <extension
    point = "org.eclipse.ui.newWizards">
    <category
      id = "org.eclipse.emf.ecore.Wizard.category.ID"
      name="%_UI_Wizard_category">
    </category>
    <wizard
      id = "de.uka.ipd.sdq.context.usage.presentation.UsageModelWizardID"
      name = "%_UI_UsageModelWizard_label"
      class = "de.uka.ipd.sdq.context.usage.presentation.UsageModelWizard"
      category = "org.eclipse.emf.ecore.Wizard.category.ID"
      icon = "icons/full/obj16/UsageModelFile.gif">
      <description>%_UI_UsageModelWizard_description</description>
      <selection class = "org.eclipse.core.resources.IResource" />
    </wizard>
  </extension>

  <extension point = "org.eclipse.ui.editors">
    <editor
        id = "de.uka.ipd.sdq.context.usage.presentation.UsageEditorID"
        name = "%_UI_UsageEditor_label"
        icon = "icons/full/obj16/UsageModelFile.gif"
        extensions = "usage"
        class = "de.uka.ipd.sdq.context.usage.presentation.UsageEditor" 
        contributorClass="de.uka.ipd.sdq.context.usage.presentation.UsageActionBarContributor" >
    </editor>
  </extension>    

  <extension
    point = "org.eclipse.ui.newWizards">
    <category
      id = "org.eclipse.emf.ecore.Wizard.category.ID"
      name="%_UI_Wizard_category">
    </category>
    <wizard
      id = "de.uka.ipd.sdq.context.actual_allocation.presentation.Actual_AllocationModelWizardID"
      name = "%_UI_Actual_AllocationModelWizard_label"
      class = "de.uka.ipd.sdq.context.actual_allocation.presentation.Actual_AllocationModelWizard"
      category = "org.eclipse.emf.ecore.Wizard.category.ID"
      icon = "icons/full/obj16/Actual_AllocationModelFile.gif">
      <description>%_UI_Actual_AllocationModelWizard_description</description>
      <selection class = "org.eclipse.core.resources.IResource" />
    </wizard>
  </extension>

  <extension point = "org.eclipse.ui.editors">
    <editor
        id = "de.uka.ipd.sdq.context.actual_allocation.presentation.Actual_AllocationEditorID"
        name = "%_UI_Actual_AllocationEditor_label"
        icon = "icons/full/obj16/Actual_AllocationModelFile.gif"
        extensions = "actual_allocation"
        class = "de.uka.ipd.sdq.context.actual_allocation.presentation.Actual_AllocationEditor" 
        contributorClass="de.uka.ipd.sdq.context.actual_allocation.presentation.Actual_AllocationActionBarContributor" >
    </editor>
  </extension>    
</plugin>
